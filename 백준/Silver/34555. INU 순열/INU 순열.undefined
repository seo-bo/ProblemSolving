#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

int main(void)
{
	cin.tie(0)->sync_with_stdio(0);
	int n = 0, cnt = 1;
	cin >> n;
	vector<int>v(n);
	for (int i = n - 1; i >= 0; i -= 2)
	{
		v[i] = cnt++;
	}
	for (int i = (n & 1); i < n; i += 2)
	{
		v[i] = cnt++;
	}
	for (auto& i : v)
	{
		cout << i << ' ';
	}
	return 0;
}