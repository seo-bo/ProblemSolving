#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

int main(void)
{
	cin.tie(0)->sync_with_stdio(0);
	int n = 0, t = 0;
	cin >> n;
	int a = n / 2, b = a + 1;
	while (b <= n || a >= 1)
	{
		if (!t)
		{
			cout << b++ << ' ';
		}
		else
		{
			cout << a-- << ' ';
		}
		t = (t + 1) % 2;
	}
	return 0;
}